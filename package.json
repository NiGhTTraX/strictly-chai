{
  "name": "strictly-chai",
  "version": "0.0.1",
  "description": "A statically typed subset of chai assertions written in TypeScript",
  "keywords": [
    "assert",
    "expect",
    "chai",
    "testing",
    "typed",
    "typescript",
    "bdd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NiGhTTraX/strictly-chai.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc --declaration --outDir ./dist",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext ts .",
    "test": "mocha --opts tests/mocha.opts",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "dependencies": {
    "chai": "~4.1.2",
    "sinon-chai": "~3.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0-beta.47",
    "@babel/preset-env": "~7.0.0-beta.47",
    "@babel/preset-typescript": "~7.0.0-beta.47",
    "@babel/register": "~7.0.0-beta.47",
    "@nighttrax/eslint-config": "~1.0.1",
    "@types/chai": "~4.1.3",
    "@types/mocha": "~5.1.0",
    "@types/node": "~9.6.5",
    "@types/sinon": "~4.3.1",
    "@types/sinon-chai": "~2.7.29",
    "eslint": "~4.19.1",
    "eslint-import-resolver-babel-module": "~4.0.0",
    "eslint-plugin-import": "~2.11.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.7.0",
    "eslint-plugin-typescript": "~0.12.0",
    "husky": "~0.14.3",
    "lint-staged": "~7.0.5",
    "mocha": "~5.1.1",
    "sinon": "~5.0.7",
    "ts-node": "~6.0.0",
    "typescript": "~2.8.1",
    "typescript-eslint-parser": "~15.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
